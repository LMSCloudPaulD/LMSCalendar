{
  "name": "@lms-cloud/calendar",
  "private": true,
  "version": "1.0.4",
  "description": "A calendar component.",
  "main": "lms-calendar.js",
  "module": "lms-calendar.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rimraf lms-calendar.{d.ts,d.ts.map,js,js.map} test/lms-calendar.{d.ts,d.ts.map,js,js.map} test/lms-calendar_test.{d.ts,d.ts.map,js,js.map}",
    "lint": "npm run lint:lit-analyzer && npm run lint:eslint",
    "lint:eslint": "eslint 'src/**/*.ts'",
    "lint:lit-analyzer": "lit-analyzer",
    "format": "prettier \"**/*.{cjs,html,js,json,md,ts}\" --ignore-path ./.eslintignore --write",
    "docs": "npm run docs:clean && npm run build && npm run analyze && npm run docs:build && npm run docs:assets && npm run docs:gen",
    "docs:clean": "rimraf docs",
    "docs:gen": "eleventy --config=.eleventy.cjs",
    "docs:gen:watch": "eleventy --config=.eleventy.cjs --watch",
    "docs:build": "rollup -c --file docs/lms-calendar.bundled.js",
    "docs:assets": "cp node_modules/prismjs/themes/prism-okaidia.css docs/",
    "docs:serve": "wds --root-dir=docs --node-resolve --watch",
    "analyze": "cem analyze --litelement --globs \"src/**/*.ts\"",
    "analyze:watch": "cem analyze --litelement --globs \"src/**/*.ts\" --watch",
    "serve": "wds --watch",
    "serve:prod": "MODE=prod npm run serve",
    "test": "npm run test:dev && npm run test:prod",
    "test:dev": "wtr",
    "test:watch": "wtr --watch",
    "test:prod": "MODE=prod wtr",
    "test:prod:watch": "MODE=prod wtr --watch",
    "checksize": "rollup -c ; cat lms-calendar.bundled.js | gzip -9 | wc -c ; rm lms-calendar.bundled.js"
  },
  "keywords": [
    "web-components",
    "lit-element",
    "typescript",
    "lit"
  ],
  "author": "Paul Derscheid <paul.derscheid@lmscloud.de>",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@juggle/resize-observer": "^3.4.0",
    "@lit/localize": "^0.11.4",
    "@types/luxon": "^3.4.0",
    "lit": "^3.1.1",
    "luxon": "^3.4.4"
  },
  "devDependencies": {
    "@11ty/eleventy": "^1.0.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "^4.2.0",
    "@custom-elements-manifest/analyzer": "^0.6.9",
    "@lit/localize-tools": "^0.6.10",
    "@open-wc/testing": "^3.2.2",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@rollup/plugin-replace": "^4.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@web/dev-server": "^0.1.38",
    "@web/dev-server-legacy": "^1.0.3",
    "@web/rollup-plugin-copy": "^0.3.0",
    "@web/rollup-plugin-html": "^1.11.1",
    "@web/test-runner": "^0.13.31",
    "@web/test-runner-playwright": "^0.8.10",
    "@webcomponents/webcomponentsjs": "^2.8.0",
    "eslint": "^8.56.0",
    "lit-analyzer": "^1.2.1",
    "prettier": "^2.8.8",
    "rimraf": "^3.0.2",
    "rollup": "^2.79.1",
    "rollup-plugin-minify-template-literals": "^1.1.4",
    "rollup-plugin-summary": "^1.4.3",
    "tslib": "^2.6.2",
    "typescript": "~4.7.4"
  },
  "customElements": "custom-elements.json"
}
